  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>C:\Users\youss\Desktop\WorkSpace\Projects\Published products\YiSoTranslator.JsonProvider\YiSoTranslator.JsonProvider\bin\Release\netstandard2.0\YiSoTranslator.JsonProvider.xml</DocumentationFile>
